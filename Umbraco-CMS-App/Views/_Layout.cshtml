@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
	Layout = null;
}



<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    @if(!string.IsNullOrEmpty(ViewData["PageDescription"].ToString())) { <meta name="description" content="@ViewData["PageDescription"]"> }
    
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <script src="https://kit.fontawesome.com/41c8b474e2.js" crossorigin="anonymous"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous">
    <link href="css/style.css" rel="stylesheet">
    <title>@ViewData["PageTitle"] | Fixxo.</title>
</head>
<body>
    
<partial name="testnav" />

    <header class="umbraco-background">
        <section class="middle container">
            <img src="/images/img-2.svg" class="image-left" alt="middle-img-1">
            <div class="middle-body">
                <h1>SALE UP</h1>
                <h1>To 50% Off</h1>
                <p>Online shopping free home delivery over $100</p>
                <a class="Umbraco-btn" href="#">
                    SHOP NOW
                </a>
            </div>
            <img src="/images/img-1.svg" class="image-right" alt="middle-img-2">
        </section>
    </header>

    @RenderBody()

    <partial name="footer" />
    
<script src="/scripts/script.js"></script>
</body>
</html>